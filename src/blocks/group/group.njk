<h2 class="group-header">Поклонники Влодимира Влодимировича</h2>
{% for item in announce %}
<div class="group-desc sh-block sh-hidden">
    {% include '../textview/textview.njk' %}
    <div class="center"></div>
</div>
{% endfor %}
{% include '../photo/photo.njk' %}
<div class="discussions">
    <h3 class="discussions-header">Обсуждения</h3>
    {% for item in discuss %}
        <span class="date">Сегодня</span>
        <div class="discussions-item sh-block sh-hidden">            
            {% include '../textview/textview.njk' %}
            <div class="comments sh-block sh-hidden">
                <div class="counter-item show-text">
                    <div class="img"></div>
                    <span class="comments-counter">x 1488</span>
                </div>
                <h4 class="caption">Комментарии</h4>
                <div class="comments-item"></div>
            </div>            
        </div>
    {% endfor %}
    <div class="comment-send">
        <p class="comment-send-caption">Оставьте комментарий</p>
        <form action="POST">
            <input type="text" placeholder="Как к вам обращаться" class="borders">
            <textarea class="borders"></textarea>
            <button class="button borders" type="submit">отправить</button>
        </form>
    </div>
</div>