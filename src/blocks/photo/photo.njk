<div class="photoalbums section">
    {% if mainHeader %}
        <h2 class="header">{{mainHeader}}</h2>
    {% endif %}
    <h3 class="header">{{photo.header}}</h3>
    <div class="albums">
        {% for item in photo.album %}
        <div class="album-item item">
            <img class="img-prev" src="{{item.src}}" alt="{{item.alt}}">
            <div class="popup"></div>
            <div class="open-img">
                <img src="./images/space/expand.png">
            </div>
            <div class="counter">x {{item.counter}}</div>
            <a class="album-item-name" href="{{item.href}}">{{item.albumName}}</a>
        </div>
        {% endfor %}
    </div>
    <div class="center"></div>
</div>