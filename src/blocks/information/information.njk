<h2 class="header">{{information.informationName}}</h2>
<div class="basic-informtaion center">
    <h3 id="name1" class="header">{{information.subheader1}}</h3>
    {% for item in information.info_item %}
        <div class="info-item">
            <span>{{item.caption}}</span>
            {% for item in item.info %}
                <p>{{item.key}}</p>
                {% if item.link %}
                    <a href="/">{{item.link}}</a>
                {% endif %}
            {% endfor %}
        </div>
    {% endfor %}
    <h3 class="header subheader">{{information.subheader1_1}}</h3>
    {% for item in information.school_item %}
        <div class="info-item">
            <span>{{item.caption}}</span>
            <p>{{item.info}}</p>
            {% if item.link %}
                <a href="{{item.link}}">{{item.link}}</a>
            {% endif %}
        </div>
    {% endfor %}
</div>
<div class="structure center">
    <h3 id="name2" class="header">{{information.subheader2}}</h3>
    <div class="sub">
        <span class="name">{{information.unit_block}}</span><span class="grayline"></span>
    </div>
    {% for item in information.unit_item %}
        <div class="units-item">
            <h3 class="header subheader">{{item.subheader2_1}}</h3>
            {% for item in item.unit_info %}
                <div class="info-item">
                    <span>{{item.caption}}</span>
                    <p>{{item.info}}</p>
                    {% if item.link %}
                        <a href="{{item.link}}">{{item.link}}</a>
                    {% endif %}
                </div>
            {% endfor %}
        </div>
    {% endfor %}
    <div class="sub">
        <span class="name">{{information.governing_block}}</span><span class="grayline"></span>
    </div>
    {% for item in information.governing_item %}
        <div class="governing-item">
            <h3 class="header subheader">{{item.subheader2_1}}</h3>
            {% for item in item.governing_info %}
                <div class="info-item">
                    <span>{{item.caption}}</span>
                    <p>{{item.info}}</p>
                    {% if item.link %}
                        <a href="{{item.link}}">{{item.link}}</a>
                    {% endif %}
                </div>
            {% endfor %}
        </div>
    {% endfor %}
</div>
